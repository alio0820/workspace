$(document).ready(
		function(){
			$('input').focus(
					function(){			
						alert('focus');			
						$(this).addClass('tmpFocused');
					}
		);
		$('input').blur(
				function() {
					alert('foc');
					$(this).removeClass('tmpFocused');
				}
		);
		$('input').trigger('focus');
		
		}
	);
				