function isDaylightSaving(d,strDS) \{\par
\par
   if(strDS == '') return(false);\par
\par
   var m1,n1,w1,t1;\par
   var m2,n2,w2,t2;\par
   with (Math)\{\par
      m1 = floor(strDS.substr(0,2))-1;\par
      w1 = floor(strDS.substr(3,1));\par
      t1 = floor(strDS.substr(4,1));\par
      m2 = floor(strDS.substr(6,2))-1;\par
      w2 = floor(strDS.substr(9,1));\par
      t2 = floor(strDS.substr(10,1));\par
   \}\par
\par
   switch(strDS.substr(2,1))\{\par
      case 'F': n1=1; break;\par
