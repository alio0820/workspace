      this.sMonth     = sMonth;  //\'b9\'ab\'d4\'aa\'d4\'c2\'ca\'fd\'d7\'d6\par
      this.sDay       = sDay;    //\'b9\'ab\'d4\'aa\'c8\'d5\'ca\'fd\'d7\'d6\par
      this.week       = week;    //\'d0\'c7\'c6\'da, 1\'b8\'f6\'d6\'d0\'ce\'c4\par
      //\'c5\'a9\'c0\'fa\par
      this.lYear      = lYear;   //\'b9\'ab\'d4\'aa\'c4\'ea4\'ce\'bb\'ca\'fd\'d7\'d6\par
      this.lMonth     = lMonth;  //\'c5\'a9\'c0\'fa\'d4\'c2\'ca\'fd\'d7\'d6\par
      this.lDay       = lDay;    //\'c5\'a9\'c0\'fa\'c8\'d5\'ca\'fd\'d7\'d6\par
      this.isLeap     = isLeap;  //\'ca\'c7\'b7\'f1\'ce\'aa\'c5\'a9\'c0\'fa\'c8\'f2\'d4\'c2?\par
      //\'b0\'cb\'d7\'d6\par
      this.cYear      = cYear;   //\'c4\'ea\'d6\'f9, 2\'b8\'f6\'d6\'d0\'ce\'c4\par
      this.cMonth     = cMonth;  //\'d4\'c2\'d6\'f9, 2\'b8\'f6\'d6\'d0\'ce\'c4\par
      this.cDay       = cDay;    //\'c8\'d5\'d6\'f9, 2\'b8\'f6\'d6\'d0\'ce\'c4\par
\par
      this.color      = '';\par
\par
      this.lunarFestival = ''; //\'c5\'a9\'c0\'fa\'bd\'da\'c8\'d5\par
      this.solarFestival = ''; //\'b9\'ab\'c0\'fa\'bd\'da\'c8\'d5\par
