      if((i+1)==firstNode) cM = cyclical((y-1900)*12+m+13);\par
      //\'c8\'d5\'d6\'f9\par
      cD = cyclical(dayCyclical+i);\par
\par
      //sYear,sMonth,sDay,week,\par
      //lYear,lMonth,lDay,isLeap,\par
      //cYear,cMonth,cDay\par
      this[i] = new calElement(y, m+1, i+1, nStr1[(i+this.firstWeek)%7],\par
