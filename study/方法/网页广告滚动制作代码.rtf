//====================================== \'b7\'b5\'bb\'d8\'c5\'a9\'c0\'fa y\'c4\'ea\'b5\'c4\'d7\'dc\'cc\'ec\'ca\'fd\par
function lYearDays(y) \{\par
 var i, sum = 348;\par
 for(i=0x8000; i>0x8; i>>=1) sum += (lunarInfo[y-1900] & i)? 1: 0;\par
 return(sum+leapDays(y));\par
\}\par
\par
//====================================== \'b7\'b5\'bb\'d8\'c5\'a9\'c0\'fa y\'c4\'ea\'c8\'f2\'d4\'c2\'b5\'c4\'cc\'ec\'ca\'fd\par
function leapDays(y) \{\par
 if(leapMonth(y)) return( (lunarInfo[y-1899]&0xf)==0xf? 30: 29);\par
 else return(0);\par
\}\par
\par
//====================================== \'b7\'b5\'bb\'d8\'c5\'a9\'c0\'fa y\'c4\'ea\'c8\'f2\'c4\'c4\'b8\'f6\'d4\'c2 1-12 , \'c3\'bb\'c8\'f2\'b7\'b5\'bb\'d8 0\par
function leapMonth(y) \{\par
 var lm = lunarInfo[y-1900] & 0xf;\par
 return(lm==0xf?0:lm);\par
\}\par
\par
//====================================== \'b7\'b5\'bb\'d8\'c5\'a9\'c0\'fa y\'c4\'eam\'d4\'c2\'b5\'c4\'d7\'dc\'cc\'ec\'ca\'fd\par
function monthDays(y,m) \{\par
 return( (lunarInfo[y-1900] & (0x10000>>m))? 30: 29 );\par
\}\par
\par
\par
//====================================== \'cb\'e3\'b3\'f6\'c5\'a9\'c0\'fa, \'b4\'ab\'c8\'eb\'c8\'d5\'c6\'da\'bf\'d8\'bc\'fe, \'b7\'b5\'bb\'d8\'c5\'a9\'c0\'fa\'c8\'d5\'c6\'da\'bf\'d8\'bc\'fe\par
//                                       \'b8\'c3\'bf\'d8\'bc\'fe\'ca\'f4\'d0\'d4\'d3\'d0 .year .month .day .isLeap\par
function Lunar(objDate) \{\par
\par
   var i, leap=0, temp=0;\par
   var offset   = (Date.UTC(objDate.getFullYear(),objDate.getMonth(),objDate.getDate()) - Date.UTC(1900,0,31))/86400000;\par
\par
   for(i=1900; i<2100 && offset>0; i++) \{ temp=lYearDays(i); offset-=temp; \}\par
\par
   if(offset<0) \{ offset+=temp; i--; \}\par
\par
   this.year = i;\par
\par
   leap = leapMonth(i); //\'c8\'f2\'c4\'c4\'b8\'f6\'d4\'c2\par
   this.isLeap = false;\par
\par
   for(i=1; i<13 && offset>0; i++) \{\par
      //\'c8\'f2\'d4\'c2\par
      if(leap>0 && i==(leap+1) && this.isLeap==false)\par
         \{ --i; this.isLeap = true; temp = leapDays(this.year); \}\par
      else\par
         \{ temp = monthDays(this.year, i); \}\par
\par
      //\'bd\'e2\'b3\'fd\'c8\'f2\'d4\'c2\par
      if(this.isLeap==true && i==(leap+1)) this.isLeap = false;\par
\par
      offset -= temp;\par
   \}\par
\par
   if(offset==0 && leap>0 && i==leap+1)\par
      if(this.isLeap)\par
         \{ this.isLeap = false; \}\par
      else\par
         \{ this.isLeap = true; --i; \}\par
\par
   if(offset<0)\{ offset += temp; --i; \}\par
\par
   this.month = i;\par
   this.day = offset + 1;\par
\}\par
\par
//==============================\'b7\'b5\'bb\'d8\'b9\'ab\'c0\'fa y\'c4\'ea\'c4\'b3m+1\'d4\'c2\'b5\'c4\'cc\'ec\'ca\'fd\par
function solarDays(y,m) \{\par
   if(m==1)\par
      return(((y%4 == 0) && (y%100 != 0) || (y%400 == 0))? 29: 28);\par
   else\par
      return(solarMonth[m]);\par
\}\par
//============================== \'b4\'ab\'c8\'eb offset \'b7\'b5\'bb\'d8\'b8\'c9\'d6\'a7, 0=\'bc\'d7\'d7\'d3\par
function cyclical(num) \{\par
   return(Gan[num%10]+Zhi[num%12]);\par
\}\par
\par
//============================== \'d2\'f5\'c0\'fa\'ca\'f4\'d0\'d4\par
function calElement(sYear,sMonth,sDay,week,lYear,lMonth,lDay,isLeap,cYear,cMonth,cDay) \{\par
\par
      this.isToday    = false;\par
      //\'b0\'ea\'be\'e4\par
