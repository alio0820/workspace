   if(m<2) cY=cyclical(y-1900+36-1);\par
   else cY=cyclical(y-1900+36);\par
   var term2=sTerm(y,2); //\'c1\'a2\'b4\'ba\'c8\'d5\'c6\'da\par
\par
   ////////\'d4\'c2\'d6\'f9 1900\'c4\'ea1\'d4\'c2\'d0\'a1\'ba\'ae\'d2\'d4\'c7\'b0\'ce\'aa \'b1\'fb\'d7\'d3\'d4\'c2(60\'bd\'f8\'d6\'c612)\par
   var firstNode = sTerm(y,m*2) //\'b7\'b5\'bb\'d8\'b5\'b1\'d4\'c2\'a1\'b8\'bd\'da\'a1\'b9\'ce\'aa\'bc\'b8\'c8\'d5\'bf\'aa\'ca\'bc\par
   cM = cyclical((y-1900)*12+m+12);\par
\par
   //\'b5\'b1\'d4\'c2\'d2\'bb\'c8\'d5\'d3\'eb 1900/1/1 \'cf\'e0\'b2\'ee\'cc\'ec\'ca\'fd\par
   //1900/1/1\'d3\'eb 1970/1/1 \'cf\'e0\'b2\'ee25567\'c8\'d5, 1900/1/1 \'c8\'d5\'d6\'f9\'ce\'aa\'bc\'d7\'d0\'e7\'c8\'d5(60\'bd\'f8\'d6\'c610)\par
   var dayCyclical = Date.UTC(y,m,1,0,0,0,0)/86400000+25567+10;\par
\par
   for(var i=0;i<this.length;i++) \{\par
\par
      if(lD>lX) \{\par
